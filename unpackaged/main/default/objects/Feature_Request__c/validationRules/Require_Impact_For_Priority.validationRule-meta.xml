<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Require_Impact_For_Priority</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
  ISPICKVAL(Priority__c, &quot;High&quot;),
  ISBLANK(TEXT(Impact__c))
)</errorConditionFormula>
    <errorMessage>Please set an Impact before marking a request as High priority.</errorMessage>
</ValidationRule>
